    <script src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js"></script>
    <script src="https://api.mapbox.com/mapbox.js/v3.1.0/mapbox.js"></script>
    <script src="https://openlayers.org/en/v4.1.0/build/ol.js"></script>
